Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "'"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "-", 
        RowBox[{"a", " ", "y"}]}], ")"}], "cos", " ", "z"}], "-", 
     RowBox[{"b", " ", "y", " ", "sin", " ", "z"}]}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
