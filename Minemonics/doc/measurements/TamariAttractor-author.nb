   3.577472252808798*^9},
 CellID->174922293],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{
     RowBox[{"sin", " ", "z", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"c", " ", "y"}], "+", "x"}], ")"}]}], "+", 
     RowBox[{"d", " ", "y", " ", "cos", " ", "z"}]}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.57659106963666*^9, 3.576591100410285*^9}, 
   3.577472254010037*^9},
 CellID->13865708],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"z", "'"}], "=", 
    RowBox[{"e", "+", 
     RowBox[{"f", " ", "z"}], "+", 
     RowBox[{"g", " ", 
      RowBox[{
       SuperscriptBox["tan", 
        RowBox[{"-", "1"}]], "(", 
       RowBox[{
        FractionBox[
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", "u"}], ")"}], " "}], 
         RowBox[{"1", "-", "i", " "}]], "x", " ", "y"}], ")"}]}]}]}], 
   TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.5765911071497602`*^9, 3.576591137558506*^9}, {
